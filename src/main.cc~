#include <iostream>
#include "Tags.h"

int main(int argc, char *argv[])
{
	Bracketext::Main(argc, argv);
	std::wstring essai = L"ffffklllll";
	// std::wstring v = Tags::readFile("C:\\Users\\prefe\\source\\repos\\Bracketext\\Bracketext\\macros.txt");
	std::wstring essai1 = L"     ";
	std::wstring vv = Tags::Trim(essai1);
	std::wcout << L"v" << vv << L"v" << std::endl;
	wchar_t cc[2] = {L'R', L'\0'};
    std::wstring ws = std::wstring(cc);
	std::wcout << L"Hello CMake." << ws << L"Coucouk" << std::endl;
	return 0;
}

